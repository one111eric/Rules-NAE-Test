{
 "when": {
     "pattern": {
       "name": "AlarmInProgress",
       "timestamp": "?time",
       "alarmSessionId": "?id"
     }
 },
 "condition": {
   "not": {
     "pattern": {
       "alarmSessionId": "?id"
     }
      }
 },
 "actions": [
   {
     "endpoint": "http://nae.dev.i.xrs.vacsv.com/notify",
     "code": {
       "location": "?location",
       "tenant": "xh",
       "type": "ALARM_IN_PROGRESS",
       "transport": [
           "x1"
       ],
       "params": {
           "deviceid": "8987917728960591079",
           "duration": "10000",
           "timezone": "PST8PDT"
       },
       "data": {
           "env": "tps",
           "timestamp": "?time",
           "zone": ""
       }
     }
   },
   {
     "code":"Env.AddFact('', {alarmSessionId: id, ttl: 10});"
   }
 ]
}